<!doctype html>
<html data-controller="yr-site" {.if authenticatedAccount}data-authenticated-account{.end}>
    {.var @n0 externalLink.url|slugify}
    {.var @n1 collection.urlId|slugify}
    <head>
        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
        <meta name="viewport" content="initial-scale=1">

        <squarespace:navigation navigationId="nav-holds" template="nav-colors"/>

        {squarespace-headers}

        <style type="text/css">
            :root {
                --tweak-yr-color-theme-on-tone-1: var(--tweak-yr-color-theme-{@n0}{@n1}-tone-1);
                --tweak-yr-color-theme-on-tone-2: var(--tweak-yr-color-theme-{@n0}{@n1}-tone-2);
                --tweak-yr-color-theme-on-tone-3: var(--tweak-yr-color-theme-{@n0}{@n1}-tone-3);
            }
        </style>
    </head>
    <body id="{squarespace.page-id}" class="{squarespace.page-classes} yr-color-theme-{@n0}{@n1}">
        <div class="yr-loader"></div>

        <squarespace:navigation navigationId="nav-main" template="nav-main"></squarespace:navigation>

        <main class="yr-main" role="main" data-content-field="main-content">
            {squarespace.main-content}
        </main>

        {@|apply footer.block}

        <squarespace:script src="site-bundle.js" combo="false"></squarespace:script>

        {squarespace-footers}

        <script type="text/javascript">console.log('site', {@|json-pretty|html});</script>
    </body>
</html>
