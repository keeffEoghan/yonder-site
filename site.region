<!doctype html>
<html
    id="{squarespace.page-id}" class="{squarespace.page-classes}"
    data-controller="site-loader"
    {.if authenticatedAccount}data-authenticated-account{.end}
    >
    <head>
        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
        <meta name="viewport" content="initial-scale=1">

        {squarespace-headers}
    </head>
    <body>
        <header>
            {.section website}
                <h1>
                    <a href="/" data-content-field="site-title">
                        {.section logoImageUrl}
                            <img src="{logoImageUrl}?format=original" alt="{siteTitle}" title="{siteTitle}" />
                        {.or}
                            {siteTitle}
                        {.end}
                    </a>
                </h1>

                {.section siteTagLine}<p>{@}</p>{.end}
            {.end}
        </header>

        <squarespace:navigation navigationId="primary-nav" template="navigation" />

        <main role="main" data-content-field="main-content">
            {squarespace.main-content}
        </main>

        <footer>
            <squarespace:block-field id="footer-blocks" columns="12" />
        </footer>

        <squarespace:script src="site-bundle.js" combo="false" />

        {squarespace-footers}
    </body>
</html>
